# 数据库配置
# MONGODB_URI=mongodb://mongo:27017/aigame?authSource=admin&replicaSet=rs0
MONGODB_URI=mongodb://root:password@localhost:27017/aigame?authSource=admin&replicaSet=rs0

# Redis配置
# REDIS_URL=redis://redis:6379
REDIS_URL=redis://localhost:6379

# MinIO配置
MINIO_ENDPOINT=aigame-minio
MINIO_PORT=9000
MINIO_ACCESS_KEY=root
MINIO_SECRET_KEY=password
MINIO_USE_SSL=false
MINIO_PUBLIC_URL=http://localhost:9000
MINIO_INTERNAL_URL=http://localhost:9000
# MINIO_INTERNAL_URL=http://aigame-minio:9000

# JWT密钥
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# 评测服务配置
EVALUATE_APP_URL=http://localhost:8000

# 认证配置
AUTH_ORIGIN=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret-key

# 应用配置
NODE_ENV=development
NUXT_PORT=3000